set syncmodifiable=nosync
set ignorecase
set smartcase
set hlsearch
set incsearch
set cursorline
set scrolloff=5
set softtabstop=2
set tabstop=2
set shiftwidth=2

:map Y y$
set contentassistmode

nnoremap <F2> :nohlsearch<CR>
nnoremap <M-h> <C-w>h
nnoremap <M-l> <C-w>l
nnoremap <M-n> <C-w>gt
nnoremap <M-p> <C-w>gT

vmap gw :format<CR>
vmap gq :format<CR>

eclipseaction findref org.eclipse.jdt.ui.edit.text.java.search.references.in.workspace
